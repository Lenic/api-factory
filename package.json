{
  "name": "@lenic/api-factory",
  "version": "1.0.8",
  "description": "Another ajax request wrapper.",
  "main": "lib/index.js",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "scripts": {
    "prepublish": "npm run build",
    "format": "prettier --write {**/*.js,**/*.jsx,**/*.less}",
    "start": "webpack-dev-server --config example/config/webpack.config.js",
    "build": "rollup -c config/rollup.default.js && rollup -c config/rollup.engine.js && rollup -c config/rollup.xhrEngine.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lenic/api-factory.git"
  },
  "keywords": [
    "ajax",
    "api",
    "factory",
    "request",
    "fetch",
    "xhr",
    "http",
    "promise",
    "adapter"
  ],
  "author": {
    "name": "Lenic Rayleigh",
    "email": "lenic@live.cn"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lenic/api-factory/issues"
  },
  "homepage": "https://github.com/Lenic/api-factory#readme",
  "peerDependencies": {
    "@lenic/deferred": "^1.0.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "@lenic/deferred": "^1.0.1",
    "babel-loader": "^8.0.4",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.15.3",
    "querystring-es3": "^0.2.1",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "underscore": "^1.8.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "optionalDependencies": {
    "axios": "^0.18.0"
  }
}
